// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("jdbc:mariadb://localhost:3306/test")
}

model Test {
  id Int @id @default(autoincrement())
  name string
  portability Boolean
}